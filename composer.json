{
    "name": "visol/typo3-handlebars",
    "description": "This is a TYPO3 CMS extension to render native handlebars templates",
    "type": "typo3-cms-extension",
    "license": "GPL-2.0+",
    "autoload": {
        "psr-4": {
            "Visol\\Handlebars\\": "Classes/"
        }
    },
    "require": {
        "php": "^8.1",
        "typo3/cms-core": "^11.5",
        "typo3/cms-extbase": "^11.5",
        "typo3fluid/fluid": "^2.7.2",
        "zordius/lightncandy": "^1.2"
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "handlebars"
        }
    }
}

